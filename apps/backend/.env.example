#Ubicacion: DCO/apps/backend/.env.example
#Configuración de la Base de Datos PostgreSQL
DB_HOST=localhost
DB_PORT=5433
DB_USER=admin
DB_PASSWORD=password123
DB_DATABASE=dco_db
DATABASE_URL=postgres://admin:password123@localhost:5433/dco_db

#Secreto para firmar los JSON Web Tokens (JWT)
#Puedes generar uno nuevo con 'openssl rand -base64 32' en tu terminal
JWT_SECRET=

#Credenciales de Cloudinary para el almacenamiento de imágenes
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=

#Credenciales de la App de Facebook para la integración con la API de Meta
FACEBOOK_APP_ID=
FACEBOOK_APP_SECRET=

#Clave de API de Google para el modelo Gemini
GEMINI_API_KEY=

#Clave de API de OpenAI para la generación de imágenes con DALL-E (o similar)
OPENAI_API_KEY=